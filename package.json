    "autoprefixer": "^10.4.18",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "naive-ui": "^2.38.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.36",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.2.0",
