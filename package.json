    "github-markdown-css": "^5.5.1",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
